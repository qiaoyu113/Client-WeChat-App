.successIssue {
  width: 100%;
  text-align: center;
  padding: 0 105rpx;
  box-sizing: border-box;
}

.successIssue image {
  width: 136rpx;
  margin: auto;
  margin-top: 130rpx;
}

.successIssue .title {
  font-size: 44rpx;
  color: #333;
  margin: 50rpx 0;
}

.successIssue .text {
  font-size: 30rpx;
  color: #a9a9a9;
}

.successIssue .myRecommend {
  width: 100%;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  color: #fff;
  font-size: 32rpx;
  background: #FAC844;
  border-radius: 8rpx;
  margin-top:130rpx;
}

.home {
  width: 100%;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
  background: #f2f2f2;
  border-radius: 8rpx;
  color: #656565;
}
